{
  "name": "gitee",
  "displayName": "gitee",
  "description": "gitee",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.35.0"
  },
  "categories": [
    "SCM Providers"
  ],
  "activationEvents": [
    "onView:myGitee",
    "onCommand:extension.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "gitee-explorer",
          "title": "Gitee",
          "icon": "media/gitee-logo.svg"
        }
      ]
    },
    "views": {
      "gitee-explorer": [
        {
          "id": "myGitee",
          "name": "码云"
        }
      ]
    },
    "commands": [
      {
        "command": "gitee.login",
        "title": "登陆Gitee",
        "icon": {
          "light": "resources/light/login.svg",
          "dark": "resources/dark/login.svg"
        }
      },
      {
        "command": "gitee.refresh",
        "title": "刷新",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "command": "gitee.createRepos",
        "title": "创建代码库"
      },
      {
        "command": "gitee.createEntRepos",
        "title": "创建企业库"
      },
      {
        "command": "gitee.createOrgRepos",
        "title": "创建组织库"
      },
      {
        "command": "gitee.clone",
        "title": "克隆",
        "icon": {
          "light": "resources/light/clone.png",
          "dark": "resources/dark/clone.png"
        }
      },
      {
        "command": "gitee.cloneTo",
        "title": "克隆到..."
      },
      {
        "command": "gitee.setRemote",
        "title": "设为远程仓库",
        "icon": {
          "light": "resources/light/remote.png",
          "dark": "resources/dark/remote.png"
        }
      },
      {
        "command": "gitee.selectedRepos",
        "title": "选中仓库"
      },
      {
        "command": "gitee.test",
        "title": "测试"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "gitee.login",
          "when": "view == myGitee",
          "group": "navigation"
        },
        {
          "command": "gitee.refresh",
          "when": "view == myGitee",
          "group": "navigation"
        },
        {
          "command": "gitee.createRepos",
          "when": "view == myGitee"
        },
        {
          "command": "gitee.createEntRepos",
          "when": "view == myGitee"
        },
        {
          "command": "gitee.createOrgRepos",
          "when": "view == myGitee"
        },
        {
          "command": "gitee.clone",
          "when": "view == myGitee"
        },
        {
          "command": "gitee.cloneTo",
          "when": "view == myGitee"
        },
        {
          "command": "gitee.setRemote",
          "when": "view == myGitee"
        },
        {
          "command": "gitee.test",
          "when": "view == myGitee"
        },
        {
          "command": "gitee.test",
          "when": "view == myGitee"
        }
      ],
      "view/item/context": [
        {
          "command": "gitee.clone",
          "when": "view == myGitee",
          "group": "inline@1"
        },
        {
          "command": "gitee.setRemote",
          "when": "view == myGitee",
          "group": "inline@2"
        }
      ]
    },
    "configuration": [
      {
        "title": "JSON Outline",
        "properties": {
          "jsonOutline.autorefresh": {
            "type": "boolean",
            "default": false
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mkdirp": "^0.5.2",
    "@types/node": "*",
    "@types/rimraf": "^2.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode": "^1.1.35"
  },
  "dependencies": {
    "@types/jest": "^24.0.15",
    "@types/shelljs": "^0.8.5",
    "axios": "^0.19.0",
    "execa": "^2.0.3",
    "fs": "^0.0.1-security",
    "ftp": "^0.3.10",
    "jsonc-parser": "^0.4.2",
    "mkdirp": "^0.5.1",
    "path": "^0.12.7",
    "rimraf": "^2.6.2",
    "sander": "^0.6.0",
    "shelljs": "^0.8.3",
    "tsdi": "^0.20.3"
  }
}
